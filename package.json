{
  "name": "dns-dev",
  "version": "1.0.0",
  "description": "Simple dev DNS server, work with docker and traefik",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon ./server.js",
    "build": "pkg server.js --targets node16-linux-x64,node16-win-x64 --compress --output ./dist/dev-dns"
  },
  "author": {
    "name": "Georgy Malkov",
